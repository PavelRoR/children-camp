*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: F
	color #000000
	background-color #fff
	line-height: 
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display block
	text-align center
	margin 0 auto
	text-decoration none
	color: #000
	background-color: #f6a354
	background-image linear-gradient(to bottom, #f6a354, #f49b46)
	width: 100%
	max-width: 331px;
	border-radius: 10px;
	box-shadow: 0px 1px 9.6px 0.4px rgba(244, 155, 70, 0.004),inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57), 0 1px 10px #f49b46
	padding: 20px 10px
	font-size: 20px
	font-family: FB
	font-weight: bold
	transition: all .3s ease
	&:focus
		text-decoration none
		color #000
	&:hover
		text-decoration none
		color #000
		background-image linear-gradient(to top, #f6a354, #f49b46)
		box-shadow: 0px 1px 9.6px 0.4px rgba(244, 155, 70, 0.004),inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57), 0 1px 4px #f49b46
		transform: translateY(3px)
.general-title
	font-size: 45px
	font-family: FB
	font-weight: bold
	color #330e0b
	margin-bottom: 45px