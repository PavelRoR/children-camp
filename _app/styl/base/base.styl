*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: F
	color #000000
	background-color #fff
	line-height: 
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.laptop-wrapper
	position relative
	z-index 50
	&:before
		content ''
		display block
		width 100%
		height 121%
		padding-top: 31.25%
		margin 0 auto
		top -1%
		left -1px
		right 0
		position absolute
		background-image url(../img/images/laptop.png)
		background-repeat no-repeat
		background-position: center
		background-size cover
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 5px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.video-wrapper-laptop
	margin 0 auto
	position relative
	width 90%
	top 0
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 85%
		height 100%
		display block
		background-color #000
		border-radius 5px
		border: 0
		top 3%
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display block
	text-align center
	margin 0 auto
	text-decoration none
	color: #000
	background-color: #f6a354
	background-image linear-gradient(to bottom, #f6a354, MO)
	width: 100%
	max-width: 331px;
	border-radius: 10px;
	box-shadow: 0px 1px 9.6px 0.4px rgba(244, 155, 70, 0.004),inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57), 0 1px 10px MO
	padding: 20px 10px
	font-size: 20px
	font-family: FB
	font-weight: bold
	transition: all .3s ease
	&:focus
		text-decoration none
		color #000
	&:hover
		text-decoration none
		color #000
		background-image linear-gradient(to top, #f6a354, MO)
		box-shadow: 0px 1px 9.6px 0.4px rgba(244, 155, 70, 0.004),inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57), 0 1px 4px MO
		transform: translateY(3px)
.general-title
	font-size: 45px
	font-family: FB
	font-weight: bold
	color #330e0b
	margin-bottom: 45px
.general-list
	li
		list-style: none
		padding-left: 60px
		margin-bottom: 13px
		position relative
		font-size: 22px
		font-family: FB
		font-weight: bold
		span
			color MO
		&:last-child
			margin-bottom: 0
		&:before
			content: '+'
			display block
			position absolute
			border-radius: 50px
			font-size: 22px
			text-align center
			width: 44px;
			height: 44px;
			line-height: 44px;
			background-color: #99F400
			box-shadow: 0px 1px 29px 4.64px rgba(#9fe052, .6);
			top 7px
			left 0
.general-list-2
	li
		padding-left: 12px
		margin-bottom: 10px
		position relative
		list-style: none
		&:before
			content '-'
			display block
			position absolute
			color MO
			top 0
			left 0
		&:last-child
			margin-bottom: 0