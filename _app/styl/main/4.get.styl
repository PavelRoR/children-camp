#get
	background-image url(../img/backs/get.png)
	background-repeat no-repeat
	background-position: center
	background-size 500px
.general-list-get
	&-1
		li
			margin-right 60px
			text-align right
			padding 0 60px 0 0
			&:before
				left auto
				right 0
			&:first-child
				margin-right: 0
	&-2
		li
			margin-left 60px
			&:first-child
				margin-left: 0
.button-get
	margin-top: 40px